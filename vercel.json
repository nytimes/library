{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server/index.js" },
      { "src": "/public/(.*)", "dest": "/public/$1" }
    ],
    "files": [
      "server/cache/store.js"
    ]

}  